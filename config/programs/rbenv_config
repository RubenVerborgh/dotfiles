# Inline version of eval "$(rbenv init - --no-rehash)"

export PATH=$HOME/.rbenv/shims:$HOME/.rbenv/bin:${PATH}

rbenv() {
  local command="$1"
  if [ "$#" -gt 0 ]; then
    shift
  fi

  case "$command" in
  shell)
    eval `rbenv "sh-$command" "$@"`;;
  *)
    command rbenv "$command" "$@";;
  esac
}
